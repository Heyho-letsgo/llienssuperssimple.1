<h1>Listing principalgroupes</h1>

<table>
  <thead>
    <tr>
      <th>Nom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @principalgroupes.each do |principalgroupe| %>
      <tr>
        <td><%= principalgroupe.nom %></td>
        <td><%= link_to 'Show', principalgroupe %></td>
        <td><%= link_to 'Edit', edit_principalgroupe_path(principalgroupe) %></td>
        <td><%= link_to 'Destroy', principalgroupe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Principalgroupe', new_principalgroupe_path %>
